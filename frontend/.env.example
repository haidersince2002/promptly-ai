# Client environment variables (copy to .env and fill in)

# Clerk publishable key (Frontend)
# Get from Clerk dashboard -> API Keys -> Publishable key
VITE_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_publishable_key

# Base URL for your backend API
# For local dev, if server runs on port 3000 use:
VITE_BASE_URL=http://localhost:3000

# Optional: Enable rendering of Clerk PricingTable component (requires billing enabled in Clerk dashboard)
VITE_ENABLE_CLERK_BILLING=false
